@model List<WPHW3.Models.Doctor>
@{
    ViewBag.Title = "PatientAddDoctor";
}

<h2>Add Doctor</h2>
@Html.ActionLink("back", "PatientMaster");
@if (Model.Count > 0)
{
    <h3>Awaible doctors</h3>
    <ul>
        @foreach (var doctor in Model)
        {
            <li>
                <h4>@doctor.FullName</h4>
                <p>@doctor.Description</p>
                @Html.ActionLink("Add", "PatientDoAddDoctor","Accounts", new { Id=doctor.Id },null)
            </li>

        }
    </ul>
}
else
{
    <p>There arent any awaible doctors</p>
}

